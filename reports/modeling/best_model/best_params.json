{
    "classifier_params": {
        "boosting_type": "gbdt",
        "colsample_bytree": 1.0,
        "importance_type": "split",
        "learning_rate": 0.1,
        "max_depth": -1,
        "min_child_samples": 20,
        "min_child_weight": 0.001,
        "min_split_gain": 0.0,
        "n_estimators": 100,
        "num_leaves": 31,
        "reg_alpha": 0.0,
        "reg_lambda": 0.0,
        "subsample": 1.0,
        "subsample_for_bin": 200000,
        "subsample_freq": 0
    },
    "balancer": "smote",
    "preprocessor": {
        "numerical_features": [
            "Age_Years",
            "Systolic_BP",
            "Diastolic_BP",
            "BMI",
            "Pulse_Pressure",
            "Demographics_Score",
            "Clinical_Biomarkers_Score",
            "Lifestyle_Score"
        ],
        "ordinal_features": [
            "Glucose_Level",
            "Cholesterol_Level",
            "BP_level"
        ],
        "ordinal_categories": [
            [
                "Normal",
                "Above Normal",
                "Well Above Normal"
            ],
            [
                "Normal",
                "Above Normal",
                "Well Above Normal"
            ],
            [
                1,
                2,
                3,
                4,
                5
            ]
        ],
        "nominal_features": [
            "Sex",
            "Smoking_Status",
            "Alcohol_Intake",
            "Physical_Activity"
        ]
    }
}